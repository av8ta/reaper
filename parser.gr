import String from "string"

export let literal = character => input => {
  let str = String.slice(0, String.length(character), input)
  if (str == character) {
    let remainder = String.slice(
      String.length(character),
      String.length(input),
      input
    )
    Ok((character, remainder))
  } else {
    Err("!match")
  }
}
